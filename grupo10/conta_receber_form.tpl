    <input type="text" name="codigoContaAReceber"/>
    <br/>
    <label>Codigo da venda: </label>
    <input type="text" name="codigoVenda"/>
    <br/>
    <label>Data do vencimento: </label>
    <input type="text" name="dataVencimento"/>
    <br/>
    <label>Data do pagamento: </label>
    <input type="text" name="dataPagamento"/>
